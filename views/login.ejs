<div class="login" id="id_login">
  <form id="id_form_login" action="/login" method="POST">
    <input
      type="text"
      name="username_input"
      id="id_username"
      placeholder="Ussername"
      autocomplete="off"
      required
    />
    <input
      type="password"
      name="password_input"
      id="id_password"
      placeholder="Passwrod"
      autocomplete="off"
      required
    />
    <button type="submit">Login</button>
  </form>
</div>
